   ],
   "source": [
    "lst = list(dnastr)\n",
    "\n",
    "for i,x in enumerate(lst):\n",
    "    if(x=='T'): lst[i]='U'\n",
    "      \n",
    "rnastr = ''.join(lst)\n",
    "print(rnastr)"
   ]
